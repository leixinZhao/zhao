        </animation>
        <animation fps="60" iterations="1" name="SBCSwitchToggleOn">
            <step duration="500" format="4s1" from="0" key="${group:Background.alpha}" offset="0" rate="linear" to="255"/>
            <step duration="500" format="4s1" from="0" key="${group:Dot.alpha}" offset="0" rate="linear" to="255"/>
            <step duration="400" format="4s1" from="0" key="${group:Dot.grd_x}" offset="0" rate="easeinout" to="21"/>
        </animation>
        <animation fps="30" iterations="1" name="exit_sport_suspend_lock">
            <step duration="1000" format="4s1" from="360" key="${app:sportSuspendLockLayer.lockanim.lockendAngle}" offset="0" rate="linear" to="0"/>
        </animation>
        <animation fps="30" iterations="1" name="switch_off_diving">
            <step duration="500" format="4s1" key="${app:divingSettingModelLayer.listReminderItem.switch.var_offset}" offset="0" rate="linear" todelta="-50"/>
        </animation>
        <animation fps="30" iterations="1" name="switch_on_diving">
            <step duration="500" format="4s1" key="${app:divingSettingModelLayer.listReminderItem.switch.var_offset}" offset="0" rate="linear" todelta="50"/>
        </animation>
    </animations>
